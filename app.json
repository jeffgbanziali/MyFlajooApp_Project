{
  "expo": {
    "name": "MyFlajooApp",
    "slug": "MyFlajooApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logos/my_flajoo2.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/123.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    },
    "plugins": [
      "@stream-io/video-react-native-sdk",
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.name.MyFlajooApp"
    },
    "bundleIdentifier": "com.example.yourapp",
    "infoPlist": {
      "NSCameraUsageDescription": "We need your permission to access the camera",
      "NSPhotoLibraryUsageDescription": "We need your permission to access the photo library",
      "NSMicrophoneUsageDescription": "We need your permission to access the microphone"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.flaj16.MyFlajooApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5c678841-2d3c-4737-b24e-714c839ec4e4"
      }
    }
  }
}
